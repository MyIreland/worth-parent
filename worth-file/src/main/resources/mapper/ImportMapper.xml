<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.worth.file.mapper.ImportMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.worth.file.domain.Import">
        <id column="id" property="id" />
        <result column="file_id" property="fileId" />
        <result column="file_url" property="fileUrl" />
        <result column="header_row" property="headerRow" />
        <result column="total_row" property="totalRow" />
        <result column="success_row" property="successRow" />
        <result column="status" property="status" />
        <result column="user_create" property="userCreate" />
        <result column="gmt_create" property="gmtCreate" />
        <result column="gmt_modify" property="gmtModify" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, file_id AS fileId, file_url AS fileUrl, header_row AS headerRow, total_row AS totalRow, success_row AS successRow, status, user_create AS userCreate, gmt_create AS gmtCreate, gmt_modify AS gmtModify
    </sql>

</mapper>
